# ----------------------------------------------------------------------
# start of Imakefile

# $XFree86: xc/lib/GL/Imakefile,v 1.44 2003/09/28 20:14:58 alanh Exp $

#
# Imake tokens which are significant to building libGL:
#
# (1 && !0) - if defined, build libs in xc/lib/GL and xc/lib/GLw
#
# 0 - if defined, build libGL with DRI capability
#
# Only zero or one of the following can be defined at once:
# GlxBuiltInGamma - if defined, build gamma_dri.so driver into libGL
# GlxBuiltInTdfx - if defined, build tdfx_dri.so driver into libGL
# GlxBuiltInMga - if defined, build mga_dri.so driver into libGL
# GlxBuiltInI810 - if defined, build i810_dri.so driver into libGL
# GlxBuiltInR128 - if defined, build r128_dri.so driver into libGL
# GlxBuiltInRadeon - if defined, build radeon_dri.so driver into libGL
# GlxBuiltInR200 - if defined, build r200_dri.so driver into libGL
# GlxBuiltInFfb - if defined, build ffb_dri.so driver into libGL
# GlxBuiltInSIS - if defined, build sis_dri.so driver into libGL
#
# 0 - automatically defined if any of the
#   GlxBuiltIn[Gamma,Tdfx,Mga,I810,R128,Radeon,R200,Ffb,SIS] tokens is
#   defined
#
# GlxBuiltInXMesa - if defined, build XMesa (Xlib Mesa driver) into libGL
#   to enable rendering to displays that lack the GLX extension.
#
# GlxDriverUsesMesa - will be defined if building a DRI driver into
#   libGL and it uses Mesa.
#
# 0 - if defined, use the SGI OpenGL Sample Implementation
#   to build the GLX server extension (not fully implemented yet).
#
# (!1 | 1) - if defined, build static libGL.a library
#
# 1 - if defined, build dynamic libGL.so library
#
# 0 - if defined, build libGL with debug info
#
# 0 - if defined, build libGL with profiling info
#
# 1.2 - dynamic library version, currently defined to be "1.2"
#
# GlxSoProf - if defined, build dynamic libGL for profiling with soprof
#
# GlxStubLib - purpose???
#
#
# Server-side tokens:
#    1
#    0

# $Xorg: Threads.tmpl,v 1.3 2000/08/17 19:41:46 cpqbld Exp $

# $XFree86: xc/config/cf/Threads.tmpl,v 3.4 2002/11/14 21:01:14 tsi Exp $

    THREADS_CFLAGS =
   THREADS_DEFINES = -DXTHREADS  -D_REENTRANT -DXUSE_MTSAFE_API -DXNO_MTSAFE_UNISTDAPI  $(LIB_MT_DEFINES)
   THREADS_LDFLAGS =
      THREADS_LIBS =

  THREADS_CXXFLAGS =
THREADS_CXXDEFINES = -DXTHREADS  -D_REENTRANT -DXUSE_MTSAFE_API -DXNO_MTSAFE_UNISTDAPI  $(LIB_MT_DEFINES)
THREADS_CXXLDFLAGS =
   THREADS_CXXLIBS =

LIBGLBUILDDIR = GL
      GLXDIRS = glx

     MESADIRS = mesa

   OSMESADIRS = mesa/OSmesa

      DRIDIRS = apple

SUBDIRS = $(MESADIRS) $(GLXDIRS) $(DRIDIRS) $(LIBGLBUILDDIR) $(OSMESADIRS) $(DRIVERDIRS)

all::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "making" all "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			 all) || exit 1; \
	done

depend::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "depending" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			 depend) || exit 1; \
	done

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

.SUFFIXES: .ii

.cc.ii:
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(_NOOP_) $*.cc > $@

.SUFFIXES: .s

.c.s:
	$(RM) $@
	 	$(CC) -S $(CFLAGS) $(_NOOP_) $*.c

.cc.s:
	$(RM) $@
	 	$(CC) -S $(CFLAGS) $(_NOOP_) $*.cc

emptyrule::

cleandir::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut   "#"*

Makefile:: $(IMAKE)

$(IMAKE) $(IMAKE).o:
	-@(cd $(IMAKESRC) && if [ -f Makefile ]; then \
	echo "checking $@ in $(IMAKESRC) first..."; $(MAKE) imakeonly; else \
	echo "bootstrapping $@ from Makefile.ini in $(IMAKESRC) first..."; \
	$(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS="$(BOOTSTRAPCFLAGS)"; fi; \
	echo "okay, continuing in $(CURRENT_DIR)")

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

$(RMAN):
	@echo "checking $(RMANBASENAME) over in $(TOP)/config/util first..."; \
	cd $(TOP)/config/util && $(MAKE) rmanonly; \
	echo "okay, continuing in $(CURRENT_DIR)"

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

html_index::
	$(HTMLINDEXCMD) $(DESTDIR)$(DOCHTMLDIR)

clean:: cleandir

distclean:: cleandir

# ----------------------------------------------------------------------
# rules for building in SUBDIRS - do not edit

install::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "installing" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			DESTDIR=$(DESTDIR) install) || exit 1; \
	done

install.man::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "installing man pages" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			DESTDIR=$(DESTDIR) install.man) || exit 1; \
	done

clean::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "cleaning" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			 clean) || exit 1; \
	done

tags::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "tagging" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			TAGS='$(TAGS)' tags) || exit 1; \
	done

$(ONESUBDIR)/Makefile:
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[n]*) executeit="no";; esac; done; \
	cd $(ONESUBDIR) && \
	if [ "$$executeit" != "no" ]; then \
		$(IMAKEPREFIX)$(IMAKE) -I$(IMAKEPREFIX)$(IRULESRC) 			  $(IMAKE_DEFINES) $(IMAKE_WARNINGS) -DTOPDIR=$(IMAKETOP) -DCURDIR=$(ONECURDIR)$(ONESUBDIR); \
	fi;

Makefiles::
	-@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[n]*) executeit="no";; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		case "$(CURRENT_DIR)" in \
		.) curdir= ;; \
		*) curdir=$(CURRENT_DIR)/ ;; \
		esac; \
		echo "making Makefiles in $$curdir$$i..."; \
		itmp=`echo $$i | sed -e 's;^\./;;g' -e 's;/\./;/;g'`; \
		curtmp="$(CURRENT_DIR)" \
		toptmp=""; \
		case "$$itmp" in \
		    ../?*) \
			while echo "$$itmp" | grep '^\.\./' > /dev/null;\
			  do \
			     toptmp="/`basename $$curtmp`$$toptmp"; \
			     curtmp="`dirname $$curtmp`"; \
			     itmp="`echo $$itmp | sed 's;\.\./;;'`"; \
			  done \
		    ;; \
		esac; \
		case "$$itmp" in \
		*/?*/?*/?*/?*)	newtop=../../../../..;; \
		*/?*/?*/?*)	newtop=../../../..;; \
		*/?*/?*)	newtop=../../..;; \
		*/?*)		newtop=../..;; \
		*)		newtop=..;; \
		esac; \
		newtop="$$newtop$$toptmp"; \
		case "$(TOP)" in \
		/?*) imaketop=$(TOP) \
		     imakeprefix= ;; \
		.) imaketop=$$newtop \
		   imakeprefix=$$newtop/ ;; \
		*) imaketop=$$newtop/$(TOP) \
		   imakeprefix=$$newtop/ ;; \
		esac; \
		$(RM) $$i/Makefile.bak; \
		if [ -f $$i/Makefile ]; then \
			echo "	$(MV) Makefile Makefile.bak"; \
			if [ "$$executeit" != "no" ]; then \
				$(MV) $$i/Makefile $$i/Makefile.bak; \
			fi; \
		fi; \
		$(MAKE) $(MFLAGS) $(MAKE_OPTS) ONESUBDIR=$$i ONECURDIR=$$curdir IMAKETOP=$$imaketop IMAKEPREFIX=$$imakeprefix $$i/Makefile; \
		if [ -d $$i ] ; then \
			cd $$i; \
			$(MAKE) $(MFLAGS) Makefiles; \
			cd $$newtop; \
		else \
			exit 1; \
		fi; \
	done

includes::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo including "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			 includes) || exit 1; \
	done

distclean::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "cleaning" "in $(CURRENT_DIR)/$$i..."; \
		(cd $$i && $(MAKE) $(MFLAGS) $(PARALLELMFLAGS) \
			 distclean) || exit 1; \
	done

distclean::
	$(RM) Makefile Makefile.dep

